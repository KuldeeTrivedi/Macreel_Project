                                                <input class="form-control sn-required2" readonly id="ClientName2" type="text" name="required" required="true" />
                                            </div>
                                        </div>
                                    </div>
                                    <div id="Clients2" style="display:none;">
                                        <h4 style="text-align:center;">
                                            Company Detail
                                        </h4>
