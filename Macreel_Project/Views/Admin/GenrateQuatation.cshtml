        text-overflow: ellipsis;
        white-space: nowrap;
        overflow: hidden;
    }

    .fs-dropdown {
        position: absolute;
        background-color: #fff;
        border: 1px solid #8a8a8a;
        width: 100%;
        margin-top: 5px;
        z-index: 1000;
    }
    .btn-primary {
        background-color: rgba(44,169,145,1) !important;
        border-color: rgba(44,169,145,1) !important;
        color: #fff !important;
        box-shadow: none !important;
